public enum ResultEnum {

    UNKNOWN_ERROR(-1,"未知错误"),
    CMD_SEND_ERROR(1,"命令发送异常")

    ;

    private Integer code;

    private String msg;

    ResultEnum(Integer code, String msg){
        this.code = code;
        this.msg = msg;
    }
    public Integer getCode(){
        return code;
    }

    public String getMsg(){
        return msg;
    }
}
